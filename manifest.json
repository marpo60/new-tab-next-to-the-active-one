{
  "name": "New tab next to the active one",
  "description": "",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "mac": "Command+MacCtrl+T"
      },
      "description": "Adds a shortcut to open a new tab next to the active one"
    }
  }
}
